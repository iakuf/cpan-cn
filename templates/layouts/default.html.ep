<!doctype html><html>
  <head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8">
    <title>
      Mojolicious - Perl real-time web framework
    </title>
    %= stylesheet '/mojo/prettify/prettify-mojo-dark.css'
    %= javascript '/mojo/prettify/run_prettify.js'
    %= stylesheet begin
      a { color: inherit }
      a:hover { color: #2a2a2a }
      a img { border: 0 }
      body {
        background: url(/mojo/pinstripe-light.png);
        color: #445555;
        font: 0.9em 'Helvetica Neue', Helvetica, sans-serif;
        font-weight: normal;
        line-height: 1.5em;
        margin: 0;
      }
      pre {
        background: url(/mojo/stripes.png);
        border: 1px solid #d1d1d1;
        border-radius: 3px;
        box-shadow: 0 1px #fff, inset -1px 1px 4px rgba(0, 0, 0, 0.1);
        color: #4d4d4c;
        font: 0.9em Consolas, Menlo, Monaco, Courier, monospace;
        line-height: 1.5em;
        padding: 1em;
        padding-bottom: 1.5em;
        padding-top: 1.5em;
        text-align: left;
        text-shadow: #eee 0 1px 0;
        white-space: pre-wrap;
      }
      #footer {
        padding-top: 1em;
        text-align: center;
      }
    %= end
    %= content 'header'
  </head>
  <body onload="prettyPrint()"><%= content %></body>
</html>

